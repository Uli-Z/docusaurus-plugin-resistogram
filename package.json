{
  "name": "docusaurus-plugin-resistogram",
  "version": "0.1.0",
  "description": "A Docusaurus plugin to display resistance data tables (resistograms) from simple CSV files.",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs"
    },
    "./remark": {
      "types": "./dist/remark/index.d.ts",
      "import": "./dist/remark/index.mjs",
      "require": "./dist/remark/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/remark/index.ts --format esm --dts && cp -r src/theme dist/",
    "dev": "tsup src/index.ts --watch",
    "prepare": "npm run build",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:e2e": "playwright test",
    "example:install": "npm install --prefix example",
    "example:start": "npm run --prefix example start",
    "example:build": "npm run --prefix example build",
    "example:serve": "npm run --prefix example serve"
  },
  "keywords": [
    "docusaurus",
    "docusaurus-plugin",
    "resistogram",
    "resistance",
    "antibiotics",
    "medicine",
    "science",
    "documentation"
  ],
  "author": "<YOUR-NAME>",
  "license": "MIT",
  "homepage": "https://github.com/<YOUR-GITHUB-USERNAME>/docusaurus-plugin-resistogram#readme",
  "bugs": {
    "url": "https://github.com/<YOUR-GITHUB-USERNAME>/docusaurus-plugin-resistogram/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<YOUR-GITHUB-USERNAME>/docusaurus-plugin-resistogram.git"
  },
  "peerDependencies": {
    "@docusaurus/core": "^3",
    "react": "^18 || ^19"
  },
  "devDependencies": {
    "@docusaurus/mdx-loader": "^3",
    "@docusaurus/types": "^3",
    "@playwright/test": "^1.54.2",
    "fs-extra": "^11.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "memfs": "^4.36.0",
    "playwright": "^1.54.2",
    "react-test-renderer": "^18.3.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@google/gemini-cli": "^0.1.9-nightly.250708.137ffec3",
    "@radix-ui/react-dropdown-menu": "^2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-tooltip": "^1",
    "csv-parse": "^5.6.0",
    "escape-string-regexp": "^5",
    "fast-glob": "^3",
    "gray-matter": "^4",
    "unist-util-visit": "^5"
  }
}
